REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# Microseconds | TESTED on i9-9900k | more queue worker = better (faster process)
MAX_LOOP_SAFETY=1000
