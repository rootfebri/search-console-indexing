APP_NAME="Search Console Bulk Indexer"
APP_ENV=production
APP_DEBUG=false
APP_TIMEZONE=UTC
APP_URL=
APP_KEY=

DB_CONNECTION=sqlite

# SESSION_DRIVER= Do not use database, it will crash because i removed migrations file
SESSION_DRIVER=file
SESSION_DOMAIN=null

# QUEUE_CONNECTION= Do not use database, it will crash because i removed migrations file
QUEUE_CONNECTION=redis

# CACHE_STORE= Do not use database, it will crash because i removed migrations file
CACHE_STORE=redis

